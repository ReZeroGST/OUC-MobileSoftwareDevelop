/* pages/index/index.wxss */
.container {
  /* 设置容器高度为视口高度 */
  height: 100vh;
  /* 使用flex布局 */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.imageStyle1 {
  /* 设置图片宽度 */
  width: 400rpx;
  /* 设置图片圆角 */
  border-radius: 50%;
}
text {
  /* 设置文本字体大小 */
  font-size: 50rpx;
  /* 设置文本颜色 */
  color: red;
}
